.container {
}

.container .top .top_tab {
  width: 100%;
  font-size: 32rpx;
  position: fixed;
  background-color: #fff;
  display: flex;
  justify-content: center;
}

.container .top .top_tab_butt {
  color: #c7c7c6;
  width: auto;
  height: 54rpx;
  line-height: 54rpx;
  padding: 0 40rpx;
  display: inline-block;
  margin: 10rpx 0 20rpx 0;
}

.container .top .checked {
  color: #ecb546;
  border-radius: 35rpx;
  box-shadow: 3rpx 6rpx 29rpx 2rpx rgba(234, 181, 74, 0.18);
}

.container .top .white_space {
  height: 110rpx;
}

